             xmlns:avaloniaUi="clr-namespace:XMLTransformer.AvaloniaUI"
             x:Class="XMLTransformer.AvaloniaUI.App">
    <Application.DataTemplates>
        <avaloniaUi:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
    </Application.Styles>
</Application>
